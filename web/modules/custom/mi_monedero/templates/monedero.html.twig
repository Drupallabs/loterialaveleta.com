{#
/**
 * @file monedero.html.twig
 * Default theme implementation to present Monedero data.
 *
 * This template is used when viewing Monedero pages.
 *
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_monedero()
 *
 * @ingroup themeable
 */
#}
<div{{ attributes.addClass('monedero') }}>
  {% if content %}
    {{- content -}}
  {% endif %}
</div>
