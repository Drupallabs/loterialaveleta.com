                <div class="row">
                <div class="col"><img alt="Euromillones" src="{{ base_path }}themes/custom/laveletav2/images/logos/euromillones.svg" /></div>
                <div class="col-md-auto">
                    <div class="botenum">
                    {% if botes.emil.0.premio_bote != 0 %}
                {{ botes.emil.0.premio_bote|number_format }} € {% else %}<span>No hay bote</span>{% endif %}
                    </div>
                    <div class="cuenta-atras"><span>{{ botes.emil.0.fecha_sorteo|diasBote|raw }}</span></div>
                </div>
                </div>
            </div>
        </article>
       
