{
    "name": "vidddd/loterialaveleta.com",
    "description": "Loteria la veleta en Drupal 8",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "david@hipertintorero.com",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "php": ">=7.0.8",
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6.5",
        "drupal-composer/drupal-scaffold": "^2.5",
        "drupal/adaptivetheme": "^3.1",
        "drupal/admin_toolbar": "^2.0",
        "drupal/admin_toolbar_mediteran": "^1.2",
        "drupal/backup_migrate": "^4.0",
        "drupal/commerce": "^2.16",
        "drupal/commerce_checkout_order_fields": "^1.1",
        "drupal/commerce_product_reminder": "^1.0@beta",
        "drupal/commerce_recurring": "^1.0@beta",
        "drupal/commerce_shipping": "^2.0@beta",
        "drupal/commerce_stock": "^1.0@alpha",
        "drupal/console": "@stable",
        "drupal/core": "^8.7.0",
        "drupal/ctools": "^3.2",
        "drupal/devel": "^2.1",
        "drupal/devel_entity_updates": "^2.0",
        "drupal/developer_module": "1.x-dev",
        "drupal/entity_clone": "^1.0@beta",
        "drupal/entity_update": "^2.0@RC",
        "drupal/examples": "^1.0",
        "drupal/fancy_login": "^2.2",
        "drupal/field_group": "^3.0",
        "drupal/form_placeholder": "^1.0",
        "drupal/htmlmail": "^3.0@alpha",
        "drupal/jquery_ui_effects": "^1.0",
        "drupal/json_field": "^1.0@RC",
        "drupal/login_popup": "^1.0",
        "drupal/mail_login": "^1.1",
        "drupal/mailsystem": "^4.2",
        "drupal/mediteran": "^1.7",
        "drupal/metatag": "^1.10",
        "drupal/mimemail": "^1.0@alpha",
        "drupal/pathauto": "^1.4",
        "drupal/rules": "^3.0@alpha",
        "drupal/security_review": "^1.0@alpha",
        "drupal/simple_sitemap": "^3.3",
        "drupal/swiftmailer": "^2.0@beta",
        "drupal/token": "^1.5",
        "drupal/twig_tweak": "^2.4",
        "drupal/video_embed_field": "^2.2",
        "drush/drush": "^9.0.0",
        "tecnickcom/tcpdf": "^6.3",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3",
        "zaporylie/composer-drupal-optimizations": "^1.0"
    },
    "require-dev": {},
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": [
            "load.environment.php"
        ]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ]
        },
        "drupal-scaffold": {
            "initial": {
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            }
        }
    }
}
